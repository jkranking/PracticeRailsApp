<%= form_for @user do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :account_id %>
  <%= f.text_field :account_id %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :is_admin %>
  <%= f.text_field :is_admin %>
  
  <%= f.submit "Save changes" %>
<% end %>